{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/images/*", "/*.json", "/*.txt", "/*.ico"]
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".json": "application/json"
  },
  "globalHeaders": {
    "cache-control": "must-revalidate, max-age=31536000"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}